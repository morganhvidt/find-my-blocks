{
  "private": true,
  "name": "find-my-blocks",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddysims/find-my-blocks.git"
  },
  "files": [
    "src/",
    "tsconfig.json",
    "package.json"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:js"
    ],
    "*.{md,mdx}": [
      "npm run lint:mdx"
    ]
  },
  "scripts": {
    "prestart": "npm run clean && node ./parcel/clean.js",
    "start": "NODE_ENV=development node ./parcel/parcel.config.js -f './src/plugin/assets/find-my-blocks.tsx'",
    "build": "NODE_ENV=production node ./parcel/parcel.config.js -f './src/plugin/assets/find-my-blocks.tsx'",
    "clean": "rm -rf ./cache",
    "lint:js": "eslint --ext js --ext jsx --ext ts --ext tsx .",
    "lint:mdx": "prettier './src/**/*.{md,mdx}' --prose-wrap always --write",
    "lint": "npm run lint:js",
    "test": "jest './src' --verbose --coverage",
    "updateBuildTags": "node ./parcel/updateBuildTags.js"
  },
  "dependencies": {
    "@types/classnames": "^2.2.10",
    "@types/lodash-es": "^4.17.3",
    "@wordpress/components": "^17.0.0",
    "classnames": "^2.2.6",
    "feather-icons-react": "^0.4.1",
    "lodash": "^4.17.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/react": "^10.0.3",
    "@types/jest": "^25.1.4",
    "@types/react": "^17.0.25",
    "@types/react-dom": "^17.0.9",
    "@types/wordpress__components": "^14.0.3",
    "@typescript-eslint/parser": "^5.0.0",
    "browser-sync": "^2.26.7",
    "chalk": "^3.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.25.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-typescript": "^0.14.0",
    "fast-glob": "^3.2.2",
    "fs-extra": "^9.0.0",
    "glob-watcher": "^5.0.3",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.4",
    "lint-staged": "^10.1.1",
    "minimist": "^1.2.5",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^1.5.0",
    "prettier": "^2.0.5",
    "typescript": "^4.4.3",
    "use-resize-observer": "^6.1.0-alpha.3"
  }
}
