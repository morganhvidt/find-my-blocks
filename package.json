{
  "private": true,
  "name": "docz-example-typescript",
  "version": "2.0.0-rc.41",
  "license": "MIT",
  "files": [
    "src/",
    "doczrc.js",
    "tsconfig.json",
    "package.json"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:js",
      "git add"
    ],
    "src/**/*.css": [
      "npm run lint:css:fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "npm run dev",
    "prewordpress": "node ./parcel/clean.js",
    "wordpress": "node ./parcel/parcel.config.js -f './src/plugin/assets/find-my-blocks.tsx' --env=development",
    "predev": "rm -rf .docz & rm -rf .cache",
    "dev": "docz dev",
    "build": "docz build",
    "lint:js": "eslint --ext js --ext jsx --ext ts --ext tsx .",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint": "npm run lint:js & npm run lint:css",
    "test": "jest './src' --verbose --coverage",
    "plop": "plop"
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@types/classnames": "^2.2.10",
    "classnames": "^2.2.6",
    "docz": "latest",
    "feather-icons-react": "^0.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.9.2",
    "browser-sync": "^2.26.7",
    "chalk": "^3.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "fast-glob": "^3.2.2",
    "fs-extra": "^9.0.0",
    "glob-watcher": "^5.0.3",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.4",
    "lint-staged": "^10.1.1",
    "minimist": "^1.2.5",
    "parcel-bundler": "^1.12.4",
    "plop": "^2.6.0",
    "postcss-modules": "^1.5.0",
    "prettier": "^2.0.2",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^12.0.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-property-sort-order-smacss": "^6.0.0",
    "stylelint-config-standard": "^19.0.0"
  }
}
