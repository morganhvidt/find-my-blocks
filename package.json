{
  "name": "find-my-blocks",
  "version": "1.0.0",
  "description": "A tool to help you find what Gutenbergs blocks you have used on your website and where they are located.",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/find-my-blocks.html",
    "lint": "eslint './src/**/*.{ts,tsx,js,jsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddysims/find-my-blocks.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eddysims/find-my-blocks/issues"
  },
  "homepage": "https://github.com/eddysims/find-my-blocks#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint -- --max-warnings=0",
      "git add"
    ]
  },
  "dependencies": {
    "parcel-bundler": "^1.12.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "typescript": "^3.7.2"
  }
}
