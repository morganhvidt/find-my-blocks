{
  "private": true,
  "name": "find-my-blocks",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddysims/find-my-blocks.git"
  },
  "files": [
    "src/",
    "tsconfig.json",
    "package.json"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:js"
    ],
    "*.{md,mdx}": [
      "npm run lint:mdx"
    ]
  },
  "scripts": {
    "prestart": "node ./parcel/clean.js",
    "start": "node ./parcel/parcel.config.js -f './src/plugin/assets/find-my-blocks.tsx' --env=development",
    "build": "node ./parcel/parcel.config.js -f './src/plugin/assets/find-my-blocks.tsx'",
    "serve": "docz serve",
    "lint:js": "eslint --ext js --ext jsx --ext ts --ext tsx .",
    "lint:mdx": "prettier './src/**/*.{md,mdx}' --prose-wrap always --write",
    "lint": "npm run lint:js",
    "test": "jest './src' --verbose --coverage",
    "updateBuildTags": "node ./parcel/updateBuildTags.js"
  },
  "dependencies": {
    "@emotion/styled": "^10.0.14",
    "@types/classnames": "^2.2.10",
    "@types/lodash-es": "^4.17.3",
    "@types/theme-ui": "^0.3.7",
    "classnames": "^2.2.6",
    "deepmerge": "^4.2.2",
    "docz": "^2.3.1",
    "feather-icons-react": "^0.4.1",
    "gatsby-plugin-google-tagmanager": "^2.3.16",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-theme-docz": "^2.3.1",
    "lodash": "^4.17.15",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.1.0",
    "theme-ui": "^0.3.1",
    "ts-xor": "^1.0.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/react": "^10.0.3",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.9.2",
    "@types/wordpress__components": "^8.5.3",
    "@wordpress/components": "^11.1.1",
    "browser-sync": "^2.26.7",
    "chalk": "^3.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-typescript": "^0.14.0",
    "fast-glob": "^3.2.2",
    "fs-extra": "^9.0.0",
    "glob-watcher": "^5.0.3",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.4",
    "lint-staged": "^10.1.1",
    "minimist": "^1.2.5",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^1.5.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "use-resize-observer": "^6.1.0-alpha.3"
  }
}
